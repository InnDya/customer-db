ges/CustomerListPage';\nimport CustomerUpdatePage from './pages/CustomerUpdatePage';\nimport LoginPage from './pages/LoginPage';\n\nfunction App() {\n  const emptyUser = { firstName: \"\", lastName: \"\", email: \"\" };\n  const [userInfo, setUserInfo] = useState(emptyUser);\n\n  useEffect(() => {\n    updateUserInfo();\n  }, []);\n\n  const updateUserInfo = () => {\n    const data = localStorage.getItem(\"userInfo\");\n    setUserInfo(data ? JSON.parse(data) : emptyUser);\n  }\n\nreturn (\n    <div>\n\n      <Header userInfo={userInfo} />\n\n      <div className=\"container\">\n\n        <Switch>\n\n          <Route path=\"/login\" render={(props) => (<LoginPage {...props} updateUserInfo={updateUserInfo} />)} />\n          <PrivateRoute path=\"/home/create\" component={CustomerCreatePage} />\n          <PrivateRoute path=\"/home/:id/edit\" component={CustomerUpdatePage} />\n          <PrivateRoute path=\"/home/:id\" component={CustomerDetailPage} />\n          <PrivateRoute path=\"/home\" component={CustomerListPage} />\n          <Redirect from=\"/\" to=\"/home\" />\n\n        </Switch>\n\n      </div>\n\n    </div>\n  );\n}\n\nexport default App;\n","/Users/innadyachkova/nackademin/customers-db/src/index.js",[],"/Users/innadyachkova/nackademin/customers-db/src/pages/LoginPage.jsx",[],"/Users/innadyachkova/nackademin/customers-db/src/pages/CustomerListPage.jsx",[],"/Users/innadyachkova/nackademin/customers-db/src/components/CustomerListItem.jsx",[],"/Users/innadyachkova/nackademin/customers-db/src/pages/CustomerDetailPage.jsx",["60","61"],"import React, { useState, useEffect } from 'react';\nimport { useHistory, Link } from 'react-router-dom';\n\n\nexport default function CustomerDetailPage(props) {\n    const customerId = props.match.params.id;\n    const [customerItem, setCustomerItem] = useState(null);\n    const history = useHistory(); \n\n    useEffect(() => {\n        getCustomerItem();\n    }, [])\n\n    function getCustomerItem() {\n        const url = `https://frebi.willandskill.eu/api/v1/customers/${customerId}/`;\n        const token = localStorage.getItem(\"token\");\n        fetch(url, {\n            headers: {\n                \"Content-Type\": \"application/json\",\n                \"Authorization\": `Bearer ${token}`\n            }\n        })\n            .then(res => res.json())\n            .then(data => setCustomerItem(data))\n    }\n\n    function deleteCustomer() {\n        const url = `https://frebi.willandskill.eu/api/v1/customers/${customerId}/`;\n        const token = localStorage.getItem(\"token\");\n        fetch(url, {\n            method: \"DELETE\",\n            headers: {\n                \"Content-Type\": \"application/json\",\n                \"Authorization\": `Bearer ${token}`\n            }\n        })\n        .then(() => history.push('/home'))\n    }\n\n    return (\n        <div> \n            <h1 className=\"mt-5 mb-5 text-center\">Customer Detail Page</h1>\n            {console.log(customerItem)}\n            {!customerItem\n                ?\n                (\n                    <span>Laddar data...</span>\n                )\n                :\n                (\n                    <div>\n                        <table className=\"table table-bordered table-hover\">\n                            <tbody>\n                                <tr>\n                                    <th>Name</th>\n                                    <td>{customerItem.name}</td>\n                                </tr>\n                                <tr>\n                                    <th>Organisation Number</th>\n                                    <td>{customerItem.organisationNr}</td>\n                                </tr>\n                                <tr>\n                                    <th>VAT Number</th>\n                                    <td>{customerItem.vatNr}</td>\n                                </tr>\n                                <tr>\n                                    <th>Reference</th>\n                                    <td>{customerItem.reference}</td>\n                                </tr>\n                                <tr>\n                                    <th>Payment Term</th>\n                                    <th>{customerItem.paymentTerm}</th>\n                                </tr>\n                                <tr>\n                                    <th>Website</th>\n                                    <td>\n                                        <a href={customerItem.website} target=\"_blank\">\n                                            {customerItem.website}\n                                        </a>\n                                    </td>\n                                </tr>\n                                <tr>\n                                    <th>Email</th>\n                                    <td>\n                                        <a href={`mailto:${customerItem.email}`}>\n                                            {customerItem.email}\n                                        </a>\n                                    </td>\n                                </tr>\n                                <tr>\n                                    <th>Phone Number</th>\n                                    <td>{customerItem.phoneNumber}</td>\n                                </tr>    \n                            </tbody>\n                        </table>\n                        <button className=\"btn btn-secondary mt-3\" onClick={deleteCustomer}>Delete Customer</button>\n                        <Link className=\"btn btn-secondary mt-3 ml-5\" to={`/home/${customerId}/edit`}>Edit Customer</Link>\n                    </div>\n                )\n            } \n        </div>\n    )\n}\n","/Users/innadyachkova/nackademin/customers-db/src/pages/CustomerCreatePage.jsx",[],"/Users/innadyachkova/nackademin/customers-db/src/pages/CustomerUpdatePage.jsx",["62"],"/Users/innadyachkova/nackademin/custome